<Application
    x:Class="MasteriesQuest.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedTheme="Dark">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush">Navy</SolidColorBrush>
            <SolidColorBrush x:Key="WindowCaptionBackground">Navy</SolidColorBrush>
            <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled">Navy</SolidColorBrush>
            <SolidColorBrush x:Key="WindowCaptionForeground">White</SolidColorBrush>
            <SolidColorBrush x:Key="WindowCaptionForegroundDisabled">Gray</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>